<?xml version="1.0"?>
<!-- Display the steer_bot model in rviz.
-->
<launch>
    <!-- Pass the steer_bot model to view_model.launch  -->
    <include file="$(find steer_bot_viz)/launch/view_model.launch">
        <arg name="model" value="$(find steer_bot_description)/urdf/steer_bot.urdf.xacro" />
    </include>
</launch>
